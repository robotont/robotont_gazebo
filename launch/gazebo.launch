<?xml version="1.0" ?>
<launch>
    <arg name="ns" default=""/>
    <arg name="world" default="worlds/empty.world" />
    <arg name="x_pos" default="0"/>
    <arg name="model" default="robotont_gazebo_nuc"/>
    <arg name="prefix" default="$(arg ns)_"/>
    <arg name="robot_name" default="robotont"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(arg world)"/>
    </include>
    <!-- topic for 3d_mapping -->
    <group if="$(eval arg('model') == 'robotont_gazebo_nuc')">
        <node name="relay" type="relay" pkg="topic_tools" args="$(arg ns)/camera/depth/image_rect_raw $(arg ns)/camera/aligned_depth_to_color/image_raw" />
        <node name="throttle_color" pkg="topic_tools" type="throttle"
        args="messages $(arg ns)/camera/color/image_raw 1.0 $(arg ns)/camera/color/image_raw_throttled" />
        <node name="throttle_depth" pkg="topic_tools" type="throttle"
        args="messages $(arg ns)/camera/depth/image_rect_raw 1.0 $(arg ns)/camera/aligned_depth_to_color/image_raw_throttled" />
    </group>
    <include file="$(find robotont_gazebo)/launch/upload_description.launch">
        <arg name="__ns" value="$(arg ns)"/>
        <arg name="model" default="$(arg model)"/>
    </include>

    <include file="$(find robotont_gazebo)/launch/spawn_urdf.launch" >
        <arg name="x_pos" value="$(arg x_pos)" />
    </include>

</launch>   

